{
  "name": "hope-foundation-charity",
  "private": true,
  "description": "A modern charity website built with Gatsby for Hope Foundation",
  "version": "1.0.0",
  "author": "Hope Foundation",
  "dependencies": {
    "gatsby": "^2.23.12",
    "gatsby-image": "^2.4.9",
    "gatsby-plugin-manifest": "^2.4.14",
    "gatsby-plugin-offline": "^3.2.13",
    "gatsby-plugin-react-helmet": "^3.3.6",
    "gatsby-plugin-sharp": "^2.6.14",
    "gatsby-source-filesystem": "^2.3.14",
    "gatsby-transformer-sharp": "^2.5.7",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "prettier": "2.0.5"
  },
  "keywords": [
    "gatsby",
    "charity",
    "nonprofit",
    "hope-foundation"
  ],
  "license": "MIT",
  "scripts": {
    "build": "node sammy.js; gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "node sammy.js; npm run serve",
    "serve": "gatsby serve -p ${PORT:-8080}",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hope-foundation/charity-website"
  },
  "bugs": {
    "url": "https://github.com/hope-foundation/charity-website/issues"
  },
  "engines": {
    "node": "^22.14.0"
  }
}
